<div id="<%= dom_id food %>">
  <p>
    <strong>Name:</strong>
    <%= food.name %>
  </p>
  <p>
    <strong>Description:</strong>
    <%= food.description %>
  </p>
  <p>
    <strong>Price:</strong>
    <%= food.price %>
  </p>
  <p>
    <strong>Quantity:</strong>
    <%= food.quantity %>
  </p>
  <p>
    <strong>Category:</strong>
    <%= food.category_id %>
  </p>
  <p>
    <strong>Image:</strong>
    <div><%= image_tag food.images.first.variant(:thumb) %></div>
  </p>
</div>
